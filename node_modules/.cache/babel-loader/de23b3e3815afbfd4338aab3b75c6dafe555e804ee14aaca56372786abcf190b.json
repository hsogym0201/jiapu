{"ast":null,"code":"import * as echarts from \"echarts/core\";\nimport { SankeyChart } from \"echarts/charts\";\nimport { TitleComponent, TooltipComponent } from \"echarts/components\";\nimport { CanvasRenderer } from \"echarts/renderers\";\n\n// 注册必要的组件\necharts.use([SankeyChart, TitleComponent, TooltipComponent, CanvasRenderer]);\nexport default {\n  name: \"SangJiTu\",\n  props: {\n    sankeyData: Object // 从父组件传递过来的桑基图数据\n  },\n  watch: {\n    sankeyData(newData) {\n      if (newData && newData.nodes.length > 0) {\n        this.updateChart(newData); // 当数据变化时更新桑基图\n      }\n    }\n  },\n  mounted() {\n    this.initChart();\n  },\n  methods: {\n    initChart() {\n      const chartDom = document.getElementById(\"SangJiTU\");\n      this.myChart = echarts.init(chartDom);\n    },\n    // 更新桑基图\n    updateChart(sankeyData) {\n      const option = {\n        title: {\n          text: \"家族迁徙路径\",\n          left: \"center\"\n        },\n        tooltip: {\n          trigger: \"item\",\n          formatter: params => {\n            if (params.dataType === \"node\") {\n              return `节点: ${params.name}<br>类别: ${params.data.category}`;\n            }\n            return `从: ${params.data.source}<br>到: ${params.data.target}<br>值: ${params.data.value}`;\n          }\n        },\n        series: [{\n          type: \"sankey\",\n          data: sankeyData.nodes,\n          links: sankeyData.links,\n          nodeWidth: 20,\n          nodeGap: 20,\n          // 控制节点间的间距\n          layoutIterations: 64,\n          emphasis: {\n            focus: \"adjacency\"\n          },\n          nodeAlign: \"center\",\n          // 让节点在水平方向居中\n          levels: [{\n            depth: 0,\n            itemStyle: {\n              color: \"#b71c1c\"\n            },\n            label: {\n              position: \"bottom\"\n            }\n          }, {\n            depth: 1,\n            itemStyle: {\n              color: \"#388e3c\"\n            },\n            label: {\n              position: \"bottom\"\n            }\n          }, {\n            depth: 2,\n            itemStyle: {\n              color: \"#1976d2\"\n            },\n            label: {\n              position: \"bottom\"\n            }\n          }, {\n            depth: 3,\n            itemStyle: {\n              color: \"#fbc02d\"\n            },\n            label: {\n              position: \"bottom\"\n            }\n          }],\n          lineStyle: {\n            color: \"source\",\n            opacity: 0.6\n          }\n        }],\n        graphic: [{\n          type: \"text\",\n          left: \"10%\",\n          top: \"5%\",\n          style: {\n            text: \"始祖原居地\",\n            fontSize: 16,\n            fontWeight: \"bold\",\n            align: \"center\"\n          }\n        }, {\n          type: \"text\",\n          left: \"30%\",\n          top: \"5%\",\n          style: {\n            text: \"始祖迁居地\",\n            fontSize: 16,\n            fontWeight: \"bold\",\n            align: \"center\"\n          }\n        }, {\n          type: \"text\",\n          left: \"50%\",\n          top: \"5%\",\n          style: {\n            text: \"始迁祖原居地\",\n            fontSize: 16,\n            fontWeight: \"bold\",\n            align: \"center\"\n          }\n        }, {\n          type: \"text\",\n          left: \"70%\",\n          top: \"5%\",\n          style: {\n            text: \"始迁祖迁居地\",\n            fontSize: 16,\n            fontWeight: \"bold\",\n            align: \"center\"\n          }\n        }]\n      };\n      this.myChart.setOption(option);\n    }\n  }\n};","map":{"version":3,"names":["echarts","SankeyChart","TitleComponent","TooltipComponent","CanvasRenderer","use","name","props","sankeyData","Object","watch","newData","nodes","length","updateChart","mounted","initChart","methods","chartDom","document","getElementById","myChart","init","option","title","text","left","tooltip","trigger","formatter","params","dataType","data","category","sou